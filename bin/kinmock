#!/usr/bin/env node

const cac = require('cac');
const config = require('./lib/config');

const cli = cac();

cli.command('*', 'Display help <default>', () => cli.showHelp());

cli.command('init', 'Create a blank setting file.', () => {
  config.create();
});

cli.command('fetch', 'Fetch are app schema files with kintone REST API.', () => {});

cli.parse();
